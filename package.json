{
  "author": {
    "name": "Alan Garny",
    "email": "a.garny@auckland.ac.nz"
  },
  "bugs": {
    "url": "https://github.com/opencor/webapp/issues"
  },
  "description": "A Web-based modelling environment, which can be used to organise, edit, simulate and analyse CellML files.",
  "homepage": "https://opencor.ws/",
  "license": "Apache-2.0",
  "main": "./out/main/index.cjs",
  "name": "opencor",
  "private": true,
  "productName": "OpenCOR",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opencor/webapp.git"
  },
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "electron-vite build",
    "build:web": "(cd ./src/renderer && vite build)",
    "clean": "rimraf dist node_modules out src/renderer/components.d.ts src/renderer/dist src/renderer/node_modules",
    "dev": "electron-vite dev --watch",
    "dev:web": "(cd ./src/renderer && vite dev)",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "package": "electron-vite build && electron-builder",
    "package:linux": "electron-vite build && electron-builder --linux",
    "package:mac": "electron-vite build && electron-builder --mac",
    "package:win": "electron-vite build && electron-builder --win",
    "publish:web": "yarn publish ./src/renderer",
    "start": "electron-vite preview",
    "start:web": "(cd ./src/renderer && vite preview)",
    "typecheck": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@primevue/themes": "^4.1.0",
    "electron-settings": "^4.0.4",
    "electron-updater": "^6.3.4",
    "libopencor": "https://opencor.ws/libopencor/downloads/libopencor-0.0.0.tgz",
    "primeicons": "^7.0.0",
    "primevue": "^4.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@primevue/auto-import-resolver": "^4.1.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/node": "^22.7.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cmake-js": "^7.3.0",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "electron-builder-squirrel-windows": "^25.0.5",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^3.4.13",
    "tailwindcss-primeui": "^0.3.4",
    "typescript": "^5.6.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.8",
    "vue": "^3.5.9",
    "vue-tsc": "^2.1.6"
  }
}
