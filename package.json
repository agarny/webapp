{
  "author": {
    "name": "Alan Garny",
    "email": "a.garny@auckland.ac.nz"
  },
  "binary": {
    "napi_versions": [
      9
    ]
  },
  "bugs": {
    "url": "https://github.com/opencor/webapp/issues"
  },
  "description": "A Web-based modelling environment, which can be used to organise, edit, simulate and analyse CellML files.",
  "homepage": "https://opencor.ws/",
  "license": "Apache-2.0",
  "main": "./out/main/index.cjs",
  "name": "opencor",
  "private": true,
  "productName": "OpenCOR",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opencor/webapp.git"
  },
  "type": "module",
  "version": "0.20250807.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  },
  "scripts": {
    "build": "electron-vite build",
    "build:web": "cd ./src/renderer && vite build",
    "clean": "node scripts/clean.js",
    "dev": "electron-vite dev --watch",
    "dev:web": "cd ./src/renderer && vite dev",
    "format": "prettier --log-level silent --write . && clang-format -i src/libopencor/src/*",
    "format:check": "prettier --check . && clang-format --dry-run -Werror src/libopencor/src/*",
    "install": "node scripts/version.js",
    "libopencor": "cmake-js build -B Release -O dist/libOpenCOR",
    "lint": "eslint --fix .",
    "release": "pnpm electron-builder",
    "release:web": "pnpm publish ./src/renderer",
    "start": "electron-vite preview",
    "start:web": "pnpm build:web && cd ./src/renderer && vite preview"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@primeuix/themes": "^1.2.3",
    "@primevue/auto-import-resolver": "^4.3.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.2.0",
    "@types/plotly.js": "^3.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.6.0",
    "@wasm-fmt/clang-format": "^20.1.7",
    "autoprefixer": "^10.4.21",
    "cmake-js": "^7.3.1",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "electron-conf": "^1.3.0",
    "electron-updater": "^6.6.2",
    "electron-vite": "^4.0.0",
    "eslint": "^9.32.0",
    "js-cookie": "^3.0.5",
    "jsonschema": "^1.5.0",
    "libopencor": "https://opencor.ws/libopencor/downloads/libopencor-0.20250805.0.tgz",
    "mathjs": "^14.6.0",
    "node-addon-api": "^8.5.0",
    "plotly.js-gl2d-dist-min": "^3.0.3",
    "prettier": "^3.6.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.7",
    "quill": "^2.0.3",
    "systeminformation": "^5.27.7",
    "tailwindcss": "^4.1.11",
    "tailwindcss-primeui": "^0.6.1",
    "ua-parser-js": "^2.0.4",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.0.6",
    "vue": "^3.5.18"
  }
}
