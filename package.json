{
  "author": {
    "name": "Alan Garny",
    "email": "a.garny@auckland.ac.nz"
  },
  "binary": {
    "napi_versions": [
      9
    ]
  },
  "bugs": {
    "url": "https://github.com/opencor/webapp/issues"
  },
  "description": "A Web-based modelling environment for organising, editing, simulating, and analysing CellML files.",
  "homepage": "https://opencor.ws/",
  "license": "Apache-2.0",
  "main": "./out/main/index.cjs",
  "name": "@opencor/opencor",
  "private": true,
  "productName": "OpenCOR",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opencor/webapp.git"
  },
  "type": "module",
  "version": "0.20250924.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  },
  "scripts": {
    "build": "electron-vite build",
    "build:web": "cd ./src/renderer && vite build",
    "clean": "node src/renderer/scripts/clean.js",
    "dev": "electron-vite dev --watch",
    "dev:web": "cd ./src/renderer && vite dev",
    "format": "prettier --log-level silent --write . && clang-format -i src/renderer/src/libopencor/src/*",
    "format:check": "prettier --check . && clang-format --dry-run -Werror src/renderer/src/libopencor/src/*",
    "libopencor": "cmake-js build -B Release -O ./dist/libOpenCOR",
    "lint": "eslint --fix .",
    "release": "pnpm electron-builder",
    "start": "electron-vite preview",
    "start:web": "pnpm build:web && cd ./src/renderer && vite preview",
    "version:new": "node src/renderer/scripts/version.js"
  },
  "peerDependencies": {
    "mathjs": "^14.6.0",
    "plotly.js-gl2d-dist-min": "^3.1.0",
    "vue": "^3.4.21"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.3",
    "@primevue/auto-import-resolver": "4.2.5",
    "@vueuse/core": "12.8.2",
    "js-cookie": "^3.0.5",
    "jsonschema": "^1.5.0",
    "primeicons": "^7.0.0",
    "primevue": "4.2.5",
    "quill": "^2.0.3",
    "systeminformation": "^5.27.10",
    "ua-parser-js": "^2.0.5",
    "vue": "3.4.21"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.5.2",
    "@types/plotly.js": "3.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "@wasm-fmt/clang-format": "^21.1.0",
    "autoprefixer": "^10.4.21",
    "cmake-js": "^7.3.1",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-conf": "^1.3.0",
    "electron-updater": "^6.6.2",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "node-addon-api": "^8.5.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "tailwindcss-primeui": "^0.6.1",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7"
  }
}
