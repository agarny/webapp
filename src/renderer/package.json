  },
  "description": "A Web-based modelling environment for organising, editing, simulating, and analysing CellML files.",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://opencor.ws/",
  "keywords": [
    "vue",
    "component",
    "opencor",
    "cellml",
    "sed-ml",
    "omex",
    "combine"
  ],
  "license": "Apache-2.0",
  "module": "dist/opencor.es.js",
  "name": "@opencor/opencor",
  "productName": "OpenCOR",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opencor/webapp.git"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/opencor.es.js"
    },
    "./style.css": "./dist/opencor.css"
  },
  "version": "0.20250904.0",
  "scripts": {
    "build": "vite build",
    "build:lib": "vite build --config vite.lib.config.ts && cp index.d.ts dist/index.d.ts",
    "clean": "node scripts/clean.js",
    "dev": "vite dev",
    "format": "prettier --log-level silent --write . && clang-format -i src/libopencor/src/*",
    "format:check": "prettier --check . && clang-format --dry-run -Werror src/libopencor/src/*",
    "lint": "eslint --fix .",
    "serve": "pnpm build && vite preview",
    "version:new": "node scripts/version.js"
  },
  "peerDependencies": {
    "mathjs": "^14.6.0",
    "plotly.js-gl2d-dist-min": "^3.1.0",
    "vue": "^3.4.21"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.3",
    "@primevue/auto-import-resolver": "4.2.5",
    "@vueuse/core": "12.8.2",
    "js-cookie": "^3.0.5",
    "jsonschema": "^1.5.0",
    "primeicons": "^7.0.0",
    "primevue": "4.2.5",
    "quill": "^2.0.3",
    "ua-parser-js": "^2.0.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.3.0",
    "@types/plotly.js": "3.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "tailwindcss-primeui": "^0.6.1",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.4"
  }
}
